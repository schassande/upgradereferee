{
    "name": "upgradereferee",
    "version": "0.9.2",
    "description": "Referee upgrade web application",
    "author": "Sebastien Chassande-Barrioz",
    "homepage": "https://upgrade.coachreferee.com",
    "repository": {
        "type": "git",
        "url": "https://github.com/schassande/upgradereferee.git"
    },
    "scripts": {
        "noproxy": "set HTTP_PROXY= && set HTTPS_PROXY=",
        "start": "cd app && ionic serve",
        "build": "cd app && ionic build --prod --service-worker",
        "deploy": "firebase deploy",
        "target-app": "firebase target:apply hosting  app referee-upgrade",
        "deploy-app": "firebase deploy --only hosting:app"
    },
    "private": true,
    "devDependencies": {
        "grunt": "^1.3.0",
        "grunt-contrib-jshint": "^3.0.0",
        "grunt-copy": "^0.1.0",
        "grunt-exec": "^3.0.0",
        "grunt-git-tag": "0.0.5",
        "grunt-markdown": "^0.7.0"
    }
}
